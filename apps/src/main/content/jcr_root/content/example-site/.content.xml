<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        jcr:title="Component Library"
        sling:resourceType="library/components/page/home-page"
        cq:designPath="/etc/designs/general"
        cq:deviceGroups="[/apps/library/mobile/groups/responsive]">
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <xs
                    jcr:primaryType="nt:unstructured"
                    title="{String}xs"
                    width="{Decimal}575"/>
                <sm
                    jcr:primaryType="nt:unstructured"
                    title="{String}sm"
                    width="{Decimal}767"/>
                <md
                    jcr:primaryType="nt:unstructured"
                    title="{String}md"
                    width="{Decimal}991"/>
                <lg
                    jcr:primaryType="nt:unstructured"
                    title="{String}lg"
                    width="{Decimal}1199"/>
                <xl
                    jcr:primaryType="nt:unstructured"
                    title="{String}xl"
                    width="{Decimal}1999"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
    <nl/>
    <en/>

</jcr:root>
