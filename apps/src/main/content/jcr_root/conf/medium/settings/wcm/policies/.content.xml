                                        cq:styleId="theme-black"
                                        cq:styleLabel="Black"
                                        jcr:primaryType="nt:unstructured"/>
                                    <item1
                                        cq:styleClasses="theme--grey"
                                        cq:styleId="theme-grey"
                                        cq:styleLabel="Grey"
                                        jcr:primaryType="nt:unstructured"/>
                                </cq:styles>
                            </item0>
                        </cq:styleGroups>
                    </policy_default_page>
                </page>
            </structure>
    <rep:policy/>
